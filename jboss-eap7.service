[Unit]
Description=JBoss EAP Systemctl script
After=NetworkManager.service

[Service]
Type=forking
ExecStart=/opt/jboss-eap/bin/init.d/jboss-eap-rhel.sh start
ExecStop=/opt/jboss-eap/bin/init.d/jboss-eap-rhel.sh stop
ExecReload=/opt/jboss-eap/bin/init.d/jboss-eap-rhel.sh restart
PIDFile=/var/run/jboss-eap/jboss-eap.pid

[Install]
WantedBy=multi-user.target
